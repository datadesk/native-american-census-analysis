         <gfc:carrierOfCharacteristics>
            <gfc:FC_FeatureAttribute>
               <gfc:memberName>
                  <gco:LocalName>GEOID</gco:LocalName>
               </gfc:memberName>
               <gfc:definition>
                  <gco:CharacterString>Tribal census tract identifier; a concatenation of American Indian Area/Alaska Native/Native Hawaiian area census code and tribal census tract code</gco:CharacterString>
               </gfc:definition>
               <gfc:cardinality gco:nilReason="unknown"/>
               <gfc:definitionReference xlink:title="U.S. Census Bureau"
                                        xlink:href="http://www.ngdc.noaa.gov/docucomp/eb139e38-ee29-4d59-b157-5e874d4420c4"/>
               <gfc:listedValue>
                  <gfc:FC_ListedValue>
                     <gfc:label gco:nilReason="inapplicable"/>
                     <gfc:definition>
                        <gco:CharacterString>The GEOID attribute is a concatenation of the American Indian Area/Alaska Native/Native Hawaiian area census code followed by the tribal census tract code. No spaces are allowed between the two codes. The American Indian Area/Alaska Native/Native Hawaiian area census code is taken from "AIANNHCE". The tribal census tract code is taken from "TTRACTCE".</gco:CharacterString>
                     </gfc:definition>
                  </gfc:FC_ListedValue>
               </gfc:listedValue>
            </gfc:FC_FeatureAttribute>
         </gfc:carrierOfCharacteristics>
