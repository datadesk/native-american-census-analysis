               <gfc:listedValue>
                  <gfc:FC_ListedValue>
                     <gfc:label gco:nilReason="inapplicable"/>
                     <gfc:definition>
                        <gco:CharacterString>Refer to the codes that appear in TTRACTCE10.</gco:CharacterString>
                     </gfc:definition>
                  </gfc:FC_ListedValue>
               </gfc:listedValue>
            </gfc:FC_FeatureAttribute>
